*,:after,:before{box-sizing:border-box;padding:0}body{font-size:1.25rem;line-height:1.5;font-family:sans-serif;text-rendering:optimizeSpeed}h1,h2,h3,h4,h5{font-family:sans-serif}h1{font-size:2em}.posts hr{margin-top:.25rem;margin-bottom:0;margin-right:.5rem;margin-left:.5rem;height:2px;border:none;border-bottom:1px #14110f;flex:1 0 1rem}main{max-width:80ch;padding:2ch;margin:auto}a,body{color:#14110f}::selection,a:focus,a:hover{background-color:#14110f;color:#fff}.meta{margin:0 0 2.5rem}.suppra{margin:3em 0 0;text-align:right}.tags::before{content:"\2022";margin-left:1rem}code,pre{color:#14110f;font-family:San Francisco Mono,Monaco,consolas,lucida console,dejavu sans mono,bitstream vera sans mono,monospace;font-size:.9em;border:1px solid #14110f}code{padding:.1rem;border:none}pre{padding:.5rem;overflow-x:auto}pre code{border:none}img{max-width:100%;border:1px solid #14110f}hr{background:#14110f;height:2px;border:0}ul{list-style-type:square}ul,ol{padding-left:1.2rem}header li,footer li{display:inline;text-transform:uppercase}header a,footer a{text-decoration:none}header ul,footer ul{justify-content:space-between;display:flex}[aria-current=page]{border:solid 2px;padding:.5rem}header,section,footer{padding:1rem 0}blockquote{border-left:5px solid #14110f;padding-left:1rem}.posts ul,header ul,footer ul{list-style:none}.posts,header ul,footer ul{padding:0}.posts li{align-items:center;display:flex;justify-content:space-between;margin-bottom:.7rem}.posts li a,.posts li div{white-space:nowrap;overflow:hidden;text-overflow:ellipsis;text-decoration:none}.posts li time{padding-left:1rem;white-space:nowrap;font-variant-numeric:tabular-nums}.hash{opacity:.25;text-decoration:none}table{border-collapse:collapse;text-align:left;width:100%}table tr{background:#fff;border-bottom:1px solid}table th,table td{padding:10px 20px}iframe{width:100%;height:500px}audio{width:100%}.visually-hidden{clip:rect(1px,1px,1px,1px);height:1px;overflow:hidden;position:absolute;width:1px}.cache{display:none}figure{text-align:center;margin:2em auto}footer{font-size:.825em}